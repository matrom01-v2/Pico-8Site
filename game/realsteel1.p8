-- logo animation
function logo_anim()
	cls()
	palt(0,true)
 --spr(1,90,50,3,3)
 
 spr(knife_sprite,knifex,knifey,2,2)
 
 spr(model_sprite,modelx,modely,3,3)

 if (fcount<100) then
 	if(fcount%3==0) then
 		model_sprite+=3
 		if(model_sprite>76) then
 			model_sprite=66
			end
		end
		
	else
		model_sprite=193
		spr(155,modelx-4,modely+4,2,2,true)
		spr(155,modelx+16,modely+4,2,2)
 end
 
 -- line draw
 if (fcount>50) then
 
 	if(knifex>10) then
 		knifex -=1
 		spr(spark_sprite,knifex+8,knifey+7)
 		
 		if(fcount%5==0)then
 			spark_sprite+=1
 		end
 		
 		if(spark_sprite>36) then
 			spark_sprite=32
			end
 	end
 	
 	line(90,75,knifex+10,knifey+15)
 	if(knifex==15) then
 		knife_sprite=131
 		spark_sprite=500
		end
				
 end --end line draw
 
 --display name
		if(fcount>100) then
 		print("one's comp games",knifex+12, knifey+5,8)
  end

  if(fcount>160) then
    print("press ❎ to start",20,40)
  end  

end -- end logo_anim
