__lua__
function _init()
 
  --game shizzle
 poke(0x5f2d, 1)
 p={}
 p.x=64
 p.y=64
 p.left=false
 enemies={}
 knives={}
 ground_knife={}
 timer=0
 cooldown=0
 difficulty=1
 score=0
 highscore=0
 dead=false
 state="intro"

 --logo shizzle
 fcount=0
 cut=false
 intro=false
	
 model={}
 model_sprite=66
 modelx=95
 modely=50
	
 knife_intro={}
 knife_sprite=196
 knifex=85
 knifey=60
	
 spark_sprite=32
	
 linex=90
 liney=75
end -- end init


function logo_anim()
	cls()
	palt(0,true)
 --spr(1,90,50,3,3)
 spr(knife_sprite,knifex,knifey,2,2)
 
 spr(model_sprite,modelx,modely,3,3)

 if (fcount<100) then
 	if(fcount%3==0) then
 		model_sprite+=3
 		if(model_sprite>76) then
 			model_sprite=66
			end
		end
		
	else
		model_sprite=193
		spr(155,modelx-4,modely+4,2,2,true)
		spr(155,modelx+16,modely+4,2,2)
 end
 
 -- line draw
 if (fcount>50) then
 
 	if(knifex>10) then
 		knifex -=1
 		spr(spark_sprite,knifex+8,knifey+7)
 		
 		if(fcount%5==0)then
 			spark_sprite+=1
 		end
 		
 		if(spark_sprite>36) then
 			spark_sprite=32
			end
 	end
 	
 	line(90,75,knifex+10,knifey+15)
 	if(knifex==15) then
 		knife_sprite=131
 		spark_sprite=500
		end
				
 end --end line draw
 
 --display name
		if(fcount>100) then
 		print("one's comp games",knifex+12, knifey+5,8)
  end

  if(fcount>110) then
    print("press ❎ to start",20,40)
  end  

end -- end logo_anim


function _update()
  if state=="intro" then
    update_menu()
  elseif state=="game" then
    update_game()
  end   
end --end update


function draw_menu()
  logo_anim()
end 

function draw_game()
  if dead==false then
    -- map
    rectfill(0,0,128,128,0)
  
    for gk in all(ground_knife) do
     spr(3,gk.x-4,gk.y-4,1,1,gk.left,false)
    end
  
    spr(1+16*(flr(timer/10)%2),p.x-4,p.y-4,1,1,p.left,false) -- draw player, alternating sprites on a timer
  
    for e in all(enemies) do
     spr(16*(flr(timer/10)%2),e.x-4,e.y-4,1,1,e.left,false) -- draw enemies on timer
    end
  
    for knf in all(knives) do
     circfill(knf.x,knf.y+4,knf.type,0)
     spr(2,knf.x-4,knf.y-4)
    end
  
    -- explosions
    for ex in all(explosions) do  
     if ex.duration>=4 then
      circfill(ex.x,ex.y,ex.radius,15)
     elseif ex.duration>=2 then
      circfill(ex.x,ex.y,ex.radius+3,9)
     elseif ex.duration>=0 then
      circ(ex.x,ex.y,ex.radius+6,4) 
     else
      del(explosions,ex)
     end
     ex.duration-=1
    end -- end explosions
  
   
    print("score: " ..score,2,2,8)
    rect(stat(32)-2,stat(33)-2,stat(32)+1,stat(33)+1,6) -- crossair
   else
    rectfill(0,0,128,128,13) -- end screen
   
  
    
    print("final score: " ..score,20,50,0)
    print("high score:  " ..highscore,20,58,0)
    print("press ❎ for new run",20,100,0)
    spr(8, 80, 45,3,3)
    if score>=highscore then
     print("new highest killstreak!",20,10,0)
     spr(5,85,31,2,2) -- crown
     spr(8, 80, 45,3,3) -- kat
    end
   end -- end else 
end -- end draw game

function update_menu()
  if btnp(❎) then
    state="game"
  end

  fcount+=1
	logo_anim()
	if(fcount>50) then
		pset(knifex,knifey+15,7)
	end
end  

function update_game()
-- movement
	palt(0,false) if dead==false then 
  if btn(⬅️) then
  p.x=mid(0,p.x-1,127)
  p.left=true
  end
  if btn(➡️) then
   p.x=mid(0,p.x+1,127)
   p.left=false
  end
  if btn(⬆️) then
   p.y=mid(0,p.y-1,127)
  end
  if btn(⬇️) then
   p.y=mid(0,p.y+1,127)
  end
  
  if stat(34)>0 and cooldown==0 then
   b={}
   b.x=p.x
   b.y=p.y
   vx=stat(32)-b.x
   vy=stat(33)-b.y
   if vy==0 then
    vy=0.01
   end
   ratio=vx/vy
   if ratio==-1 then
    ratio=-1.01
   end
   b.sx=sqrt((ratio*ratio)/(ratio*ratio+1))
   b.sy=sqrt(1-b.sx*b.sx)
   if vx<0 then
    b.sx=-1*b.sx
   end
   if vy<0 then
    b.sy=-1*b.sy
   end
   if stat(34)==1 then -- left mouse click, normal projectile
    b.type=1
    cooldown=8
   else -- large projectile, change this for abilities
    b.type=2
    cooldown=48
   end
   add(knives,b)  
  end

  cooldown=max(0,cooldown-1)
  
  -- spawns
  timer+=1
  if timer==60 then
  timer=0
   for i=0,flr(rnd(difficulty)) do
    e={}
    e.x=rnd(128)
    e.y=rnd(128)
    r=flr(rnd(4))
    if r==1 then
     e.x=-8
    elseif r==2 then
     e.x=135
    elseif r==3 then
     e.y=-8
    else
     e.y=135
    end  
    e.left=false
    add(enemies,e)  
   end 
  difficulty+=1 
  end

  -- projectiles
  for knf in all(knives) do
   knf.x+=knf.sx*3
   knf.y+=knf.sy*3
   for e in all(enemies) do
    if abs(e.x-knf.x)<4 and abs(e.y-knf.y)<4 then -- enemy in range of thrown knife
     if knf.type==1 then
      gk={}
      gk.x=e.x
      gk.y=e.y
      gk.left=e.left
      add(ground_knife,gk)
      del(enemies,e)
      score+=1
     elseif b.type==2 then -- right mouse attack
      sfx(3) 

    --   for e in all(enemies) do
    --    if abs(e.x-knf.x)<20 and abs(e.y-knf.y)<20 then
    --     gk={}
    --     gk.x=e.x
    --     gk.y=e.y
    --     gk.left=e.left
    --     add(ground_knife,gk)
    --     del(enemies,e)         
    --     score+=1      
    --    end
    --   end
     end  

     del(knives,knf)         
    end
   end
   if knf.x<0 or knf.y<0 or knf.x>128 or knf.y>128 then
    del(knives,knf)
   end
  end

  -- enemy movement
  for e in all(enemies) do
   vx=p.x-e.x
   vy=p.y-e.y
   ratio=vx/vy
   e.sx=sqrt((ratio*ratio)/(ratio*ratio+1))
   e.sy=sqrt(1-e.sx*e.sx)
   if vx<0 then
    e.sx=-1*e.sx
    e.left=true
   else
    e.left=false
   end
   if vy<0 then
    e.sy=-1*e.sy
   end
   e.x+=e.sx*0.75
   e.y+=e.sy*0.75
   if abs(e.x-p.x)<2 and abs(e.y-p.y)<2 then -- enemy contact
    dead=true 
    palt(0,false)
    if score>=highscore then
     highscore=score
    end   
   end
  end 
 else 
  if btn(❎) then   
   p={}
   p.x=64
   p.y=64
   p.left=false
   enemies={}
   knives={}
   ground_knife={}
   explosions={}
   timer=0
   cooldown=0
   difficulty=1
   score=0
   dead=false 
  end 
 end 
end  

function _draw()
 cls()

 if state=="intro" then
  draw_menu()
 elseif state=="game" then
  draw_game()

end  

end
