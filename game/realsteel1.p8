function draw_menu()
  logo_anim()
end -- end draw_menu

function _draw()
 cls()

 if state=="intro" then
  draw_menu()
 elseif state=="game" then
  draw_game()

	end  

end-- end draw

local aoe_frame_count, aoe_animating = 0, false -- るに\_(ツ)_/るに  '=' is God

function draw_game()

  if dead==false then
    -- map
    palt(0,true)
    rectfill(0,0,128,128,0)
    map(0,0,0,0,16,16)
  
    for gk in all(ground_knife) do
     spr(3,gk.x-4,gk.y-4,1,1,gk.left,false)
    end
  
    spr(1+16*(flr(timer/20)%2),p.x-4,p.y-4,1,1,p.left,false) -- draw player, alternating sprites on a timer
  
    for e in all(enemies) do
     spr(16*(flr(timer/20)%2),e.x-4,e.y-4,1,1,e.left,false) -- draw enemies on timer
    end
  
    -- shadows and knives
    for knf in all(knives) do
     circfill(knf.x,knf.y+4,knf.type,0)
     spr(give_me_knife(),knf.x-4,knf.y-4)
    end

    function give_me_knife()

      if knf_frame_count%2==0 then 
        thrown_knf_sprite+=1
      end
      
      if thrown_knf_sprite>55 then 
        thrown_knf_sprite=48
      end
      return thrown_knf_sprite
    end -- end give_me_knife  

    -- draw aoe circle --

    -- start aoe_anim
    if stat(34)==2 then
      aoe_animating = true
    end

    -- if animating active, do anim
    if aoe_animating == true then
      aoe_frame_count += 1
      circ(p.x,p.y,15,8)

      -- animate only for 30 framies
      if aoe_frame_count > 15 then
        aoe_animating = false
        aoe_frame_count = 0
      end
    end

  
    print("score: " ..score,4,4,0)
    rect(stat(32)-2,stat(33)-2,stat(32)+1,stat(33)+1,6) -- crossair
   else
    rectfill(0,0,128,128,13) -- end screen
   
  
    
    print("final score: " ..score,10,50,0)
    print("high score:  " ..highscore,10,58,0)
    print("press ❎ for new run",20,100,0)
    spr(8, 80, 45,3,3)
    if score>=highscore then
     print("new highest killstreak!",20,10,0)
     spr(5,85,31,2,2) -- crown
     spr(8, 80, 45,3,3) -- kat
    end
   end -- end else 
end -- end draw game
