--update functions

function _update60()
  if state=="intro" then
    update_menu()
  elseif state=="game" then
    update_game()
  end   
end --end update


function update_menu()
  if btnp(❎) then
    state="game"
  end

  fcount+=1
	logo_anim()
	if(fcount>50) then
		pset(knifex,knifey+15,7)
	end
end -- end update menu



function update_game()
-- movement
  knf_frame_count+=1
	palt(0,false)
 if dead==false then 
  if btn(⬅️) or btn(0,1) then
   p.x=mid(0,p.x-1,127)
   p.left=true
  end
  if btn(➡️) or btn(1,1) then
   p.x=mid(0,p.x+1,127)
   p.left=false
  end
  if btn(⬆️) or btn(2,1) then
   p.y=mid(0,p.y-1,127)
  end
  if btn(⬇️) or btn(3,1) then
   p.y=mid(0,p.y+1,127)
  end
  
  -- mouse tracking
  if stat(34)>0 and cooldown==0 then
   b={}
   b.x=p.x
   b.y=p.y
   vx=stat(32)-b.x
   vy=stat(33)-b.y
   if vy==0 then
    vy=0.01
   end
   ratio=vx/vy
   if ratio==-1 then
    ratio=-1.01
   end
   b.sx=sqrt((ratio*ratio)/(ratio*ratio+1))
   b.sy=sqrt(1-b.sx*b.sx)
   if vx<0 then
    b.sx=-1*b.sx
   end
   if vy<0 then
    b.sy=-1*b.sy
   end


   if stat(34)==1 then -- left mouse click, normal projectile
    b.type=1
    cooldown=16
    sfx(1)
    add(knives,b)

   else -- AOE passive
   -- b.type=2
    cooldown=48
    if(#ground_knife != 0) then
      shunpo()
    end  
   end
   --add(knives,b) -- add knife to table
  end

  
  -- shunpo
  function shunpo()
      local cur_knf = List.popleft(teleport_knvs)
      if cur_knf == nil then
        return
      end  
      p.x = cur_knf.x
      p.y = cur_knf.y
      del(ground_knife, cur_knf)
      sfx(0)
      for e in all(enemies) do
        if abs(e.x-p.x)<20 and abs(e.y-p.y)<20 then
         del(enemies,e)         
         score+=1      
        end
       end
  end -- end shunpo  



  cooldown=max(0,cooldown-1)
  
  -- spawns
  timer+=1
  if timer==120 then  
  timer=0 -- reset spawn timer
   for i=0,flr(rnd(difficulty)) do -- add enemies based on difficulty
    e={} --create enemy
    e.x=rnd(128)
    e.y=rnd(128)
    r=flr(rnd(4)) --get rand number between 1-4
    if r==1 then
     e.x=-8
    elseif r==2 then
     e.x=135
    elseif r==3 then
     e.y=-8
    else
     e.y=135
    end  
    e.left=false
    add(enemies,e)  
   end 
  difficulty+=1 
  end

  -- projectile knives
  for knf in all(knives) do
   knf.x+=knf.sx*2
   knf.y+=knf.sy*2
   for e in all(enemies) do
    if abs(e.x-knf.x)<4 and abs(e.y-knf.y)<4 then -- enemy in range of thrown knife
     if knf.type==1 then
      local gk={}
      gk.x=e.x
      gk.y=e.y
      gk.left=e.left
      add(ground_knife,gk)-- add ground knife to table
      List.pushleft(teleport_knvs, gk) -- push on to stacky
      del(enemies,e) -- remove enemy from table
      score+=1
     elseif b.type==2 then -- right mouse attack
      sfx(3) 
     -- Kat passive 
     -- check for enemies in range, if so, kill 
     -- enemies, do not leave knife
      for e in all(enemies) do
       if abs(e.x-knf.x)<20 and abs(e.y-knf.y)<20 then
        del(enemies,e)         
        score+=1      
       end
      end
     end  

     del(knives,knf)         
    end
   end
   if knf.x<0 or knf.y<0 or knf.x>128 or knf.y>128 then
    del(knives,knf)
   end
  end

  -- enemy movement
  for e in all(enemies) do
   vx=p.x-e.x
   vy=p.y-e.y
   ratio=vx/vy
   e.sx=sqrt((ratio*ratio)/(ratio*ratio+2))
   e.sy=sqrt(1-e.sx*e.sx)
   if vx<0 then
    e.sx=-1*e.sx
    e.left=true
   else
    e.left=false
   end
   if vy<0 then
    e.sy=-1*e.sy
   end
   e.x+=e.sx*0.5
   e.y+=e.sy*0.5
   if abs(e.x-p.x)<2 and abs(e.y-p.y)<2 then -- enemy contact
    dead=true 
    if score>=highscore then
     highscore=score
    end   
   end
  end 
 else 
  if btn(❎) then   
   p={}
   p.x=64
   p.y=64
   p.left=false
   enemies={}
   knives={}
   ground_knife={}
   aoe_attack={}
   timer=0
   cooldown=0
   difficulty=1
   score=0
   dead=false 
  end 
 end 
end --end update game
