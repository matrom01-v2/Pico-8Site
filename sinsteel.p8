	eny=e.y
	
	
	if(e.x>p.x+4) enx-=0.2 --posx
	if(e.x<p.x+4) enx+=0.2
	
	if(e.y>p.y+4) eny-=0.2 --posy
	if(e.y<p.y+4) eny+=0.2
	
	if (can_move(newx,newy)) then
		e.x=mid(0,enx,127)
		e.y=mid(0,eny,63)
	end

end --end en_movement
