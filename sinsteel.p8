function make_player()

	p={}
	p.x=21
	p.y=21
	p.sprite=4
	p.cnt=0
	p.speed=0.2
	p.flip = false
	p.animspeed=0.7
	
	p.ability=0
	

	
end

function draw_player()

	local cursprite=p.sprite

	
	spr(cursprite,p.x*8,p.y*8,1,1,p.flip,false)
	palt(0,true)
end

function movement()
	newx=p.x
	newy=p.y
	
	if(btn(â¬…ï¸)) then
	 newx-=p.speed
	 p.flip=true
	end
	
	if(btn(âž¡ï¸)) then 
		newx+=p.speed
	 p.flip=false
	end
	
	if(btn(â¬†ï¸)) newy-=p.speed
	if(btn(â¬‡ï¸)) newy+=p.speed
	
	if btn(âž¡ï¸) or
	   btn(â¬…ï¸) then
		p.cnt+=p.animspeed
		if p.cnt>1 then
			p.sprite+=1
			p.cnt=0
			if p.sprite>6 then
				p.sprite=4
			end
		end
 else
 	p.sprite=4 end		 
	
	
	if (can_move(p.x,p.y)) then
		p.x=mid(0,newx,127)
		p.y=mid(0,newy,63)
	else
	 sfx(0)
	end
	
end --end movement



